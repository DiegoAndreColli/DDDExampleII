@{
    ViewData["Title"] = "List Transfer Page";
}
@model DDDExampleII.Domain.Entities.Transfer;

<div class="row">
    <div class="col-md-10">
        <h2>List Transfer Page</h2>
        Click
        <a asp-area="" asp-controller="FundTransfer" asp-action="List">here</a>
        to test List

        <br />
        Start
        <br />

        @foreach (var item in ViewData["Transfers"] as IEnumerable<DDDExampleII.Domain.Entities.Transfer>)
        {
            <span>Transaction</span>
            @item.Id;
            <span> | From:</span>
            @item.From.Name;
            <span> | to:</span>
            @item.To.Name;
            <span>| Value: </span>
            @item.Value;
            <br>

        }

    </div>
</div>
